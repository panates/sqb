{
  "name": "@sqb/postgres",
  "description": "SQB serialization extension for PostgreSQL database",
  "version": "4.14.1",
  "author": "Panates",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -b tsconfig-build-cjs.json",
    "build:esm": "tsc -b tsconfig-build-esm.json",
    "postbuild": "cp README.md package.json ../../LICENSE ../../build/postgres",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "test": "jest",
    "cover": "jest --collect-coverage",
    "clean": "npm run clean:src | npm run clean:dist | npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/postgres",
    "clean:cover": "rimraf ../../coverage/postgres"
  },
  "dependencies": {
    "putil-promisify": "^1.10.1"
  },
  "devDependencies": {
    "postgrejs": "^2.16.0"
  },
  "peerDependencies": {
    "@sqb/builder": "^4.14.1",
    "@sqb/connect": "^4.14.1",
    "@sqb/postgres-dialect": "^4.14.1",
    "postgrejs": ">=2.15.1 <3.0.0"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Ilker Gurelli <i.gurelli@panates.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sqbjs/sqb.git",
    "directory": "packages/postgres"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "bin/",
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "sqb",
    "postgressql",
    "postgres",
    "pg",
    "sql",
    "query",
    "builder",
    "dialect",
    "serializer",
    "database"
  ]
}
