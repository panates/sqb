{
  "name": "@sqb/nestjs",
  "description": "Nestjs module for data connection using SQB",
  "version": "4.21.0",
  "author": "Panates",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "tsc --noEmit",
    "prebuild": "npm run check && npm run lint && npm run clean",
    "build": "tsc -b tsconfig-build.json",
    "postbuild": "node ../../support/postbuild.cjs",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "check": "madge --circular src/**",
    "test": "mocha",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:build && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:test": "ts-cleanup -s test --all",
    "clean:build": "rimraf build *.tsbuildinfo",
    "clean:cover": "rimraf ../../coverage/nestjs"
  },
  "dependencies": {
    "@jsopen/objects": "^2.0.2",
    "putil-varhelpers": "^1.6.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/testing": "^11.1.11",
    "expect": "^30.2.0",
    "postgrejs": "^2.22.9",
    "rxjs": "^7.8.1",
    "supertest": "^7.1.4"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7.4.0",
    "@nestjs/core": ">=7.4.0",
    "@sqb/builder": "^4.21.0",
    "@sqb/connect": "^4.21.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": ">=6.6.0"
  },
  "type": "module",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20.0"
  },
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Ilker Gurelli <i.gurelli@panates.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/sqb.git",
    "directory": "packages/nestjs"
  },
  "keywords": [
    "sqb",
    "nestjs",
    "nest"
  ],
  "publishConfig": {
    "access": "public"
  }
}