{
  "name": "@sqb/sqljs",
  "description": "SQB serialization extension for sql.js driver",
  "version": "4.0.7",
  "author": "Panates Ltd.",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqbjs/sqb.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "sql",
    "query",
    "builder",
    "dialect",
    "sqlite",
    "sql.js",
    "database"
  ],
  "dependencies": {
    "putil-promisify": "^1.8.5"
  },
  "devDependencies": {
    "@types/sql.js": "^1.4.2",
    "sql.js": "^1.5.0"
  },
  "peerDependencies": {
    "sql.js": "^1.4.0",
    "@sqb/sqlite-dialect": "^4.x.x",
    "@sqb/builder": "^4.x.x",
    "@sqb/connect": "^4.x.x"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 14.0"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "LICENSE",
    "README",
    "dist/"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "prebuild": "npm run lint && npm run clean:dist",
    "postbuild": "copyfiles package.json README.md LICENSE ../../build/sqljs",
    "clean": "ts-cleanup -s src --all & ts-cleanup -s test",
    "clean:dist": "rimraf ../../build/sqljs",
    "compile": "tsc",
    "lint": "eslint src/** --no-error-on-unmatched-pattern",
    "test": "TS_NODE_PROJECT='./test/tsconfig.json' mocha -r ts-node/register -r tsconfig-paths/register --reporter spec test/**/*.spec.ts",
    "version": "#version"
  }
}