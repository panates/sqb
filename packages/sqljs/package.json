{
  "name": "@sqb/sqljs",
  "description": "SQB serialization extension for sql.js driver",
  "version": "4.20.7",
  "author": "Panates",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "tsc --noEmit",
    "prebuild": "npm run check && npm run lint && npm run clean",
    "build": "tsc -b tsconfig-build.json",
    "postbuild": "node ../../support/postbuild.cjs",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "check": "madge --circular src/**",
    "test": "mocha",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:build && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:test": "ts-cleanup -s test --all",
    "clean:build": "rimraf build *.tsbuildinfo",
    "clean:cover": "rimraf ../../coverage/sqljs"
  },
  "dependencies": {
    "putil-promisify": "^1.10.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/sql.js": "^1.4.9",
    "expect": "^30.2.0",
    "sql.js": "^1.13.0"
  },
  "peerDependencies": {
    "@sqb/builder": "^4.20.7",
    "@sqb/connect": "^4.20.7",
    "@sqb/sqlite-dialect": "^4.20.7",
    "sql.js": "^1.13.0"
  },
  "type": "module",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20.0"
  },
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Ilker Gurelli <i.gurelli@panates.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/sqb.git",
    "directory": "packages/sqljs"
  },
  "keywords": [
    "sqb",
    "sqlite",
    "sql.js",
    "sql",
    "query",
    "builder",
    "dialect",
    "serializer",
    "database"
  ],
  "publishConfig": {
    "access": "public"
  }
}
