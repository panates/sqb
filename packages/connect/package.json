{
  "name": "@sqb/connect",
  "description": "Multi-dialect database connection framework written with TypeScript",
  "version": "4.0.1-beta.31",
  "author": "Panates Ltd.",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqbjs/sqb.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "sqb",
    "rdbms",
    "sql",
    "client",
    "dialect",
    "multi-dialect",
    "database",
    "oracle",
    "postgres",
    "pg",
    "mssql",
    "sqlite",
    "mysql"
  ],
  "dependencies": {
    "debug": "^4.3.2",
    "lightning-pool": "^3.1.3",
    "lodash": "^4.17.21",
    "putil-isplainobject": "^1.1.4",
    "putil-merge": "^3.7.0",
    "putil-promisify": "^1.8.4",
    "putil-taskqueue": "^2.5.3",
    "putil-varhelpers": "^1.6.1",
    "reflect-metadata": "^0.1.13",
    "strict-event-emitter-types": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.6",
    "@types/lodash": "^4.14.171",
    "typed-emitter": "^1.3.1"
  },
  "peerDependencies": {
    "@sqb/builder": "^4.0.1-beta.29"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "LICENSE",
    "README",
    "dist/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output"
  },
  "scripts": {
    "test": "TS_NODE_PROJECT='./test/tsconfig.json' mocha -r ts-node/register -r tsconfig-paths/register --reporter spec test/**/*.spec.ts",
    "cover": "nyc --reporter=cobertura --reporter html --reporter text npm run test",
    "build": "tsc -b tsconfig-build.json",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint src/** --no-error-on-unmatched-pattern"
  }
}
