{
  "name": "@sqb/connect",
  "description": "Multi-dialect database connection framework written with TypeScript",
  "version": "4.12.0",
  "author": "Panates",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Ilker Gurelli <i.gurelli@panates.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqbjs/sqb.git",
    "directory": "packages/connect"
  },
  "type": "module",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -b tsconfig-build-cjs.json",
    "build:esm": "tsc -b tsconfig-build-esm.json",
    "postbuild": "cp README.md package.json ../../LICENSE ../../build/connect && cp ../../package.cjs.json ../../build/connect/cjs/package.json",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "test": "jest",
    "cover": "jest --collect-coverage",
    "clean": "npm run clean:src | npm run clean:dist | npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/connectt",
    "clean:cover": "rimraf ../../coverage/connect"
  },
  "dependencies": {
    "debug": "^4.3.5",
    "fast-tokenizer": "^1.3.0",
    "lightning-pool": "^4.2.2",
    "lodash": "^4.17.21",
    "power-tasks": "^1.7.3",
    "putil-isplainobject": "^1.1.5",
    "putil-merge": "^3.12.1",
    "putil-promisify": "^1.10.1",
    "putil-varhelpers": "^1.6.5",
    "strict-typed-events": "^2.3.3",
    "ts-gems": "^3.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.5",
    "postgresql-client": "^2.11.2"
  },
  "peerDependencies": {
    "@sqb/builder": "^4.12.0",
    "reflect-metadata": "^0.2.2"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "bin/",
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "sqb",
    "rdbms",
    "sql",
    "client",
    "dialect",
    "multi-dialect",
    "database",
    "oracle",
    "postgres",
    "pg",
    "mssql",
    "sqlite",
    "mysql"
  ]
}
