{
  "name": "@sqb/migrator",
  "description": "Database migrator for SQB",
  "version": "4.21.0",
  "author": "Panates",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "tsc --noEmit",
    "prebuild": "npm run check && npm run lint && npm run clean",
    "build": "tsc -b tsconfig-build.json",
    "postbuild": "node ../../support/postbuild.cjs",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "check": "madge --circular src/**",
    "test": "mocha",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:build && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:test": "ts-cleanup -s test --all",
    "clean:build": "rimraf build *.tsbuildinfo",
    "clean:cover": "rimraf ../../coverage/migrator"
  },
  "dependencies": {
    "cross-dirname": "^0.1.0",
    "fast-glob": "^3.3.3",
    "strict-typed-events": "^2.8.0",
    "ts-gems": "^3.11.3",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@sqb/builder": "^4.21.0",
    "@sqb/connect": "^4.21.0",
    "@sqb/postgres": "^4.21.0"
  },
  "devDependencies": {
    "expect": "^30.2.0",
    "postgrejs": "^2.22.9"
  },
  "type": "module",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20.0"
  },
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Ilker Gurelli <i.gurelli@panates.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/sqb.git",
    "directory": "packages/migrator"
  },
  "keywords": [
    "sqb",
    "sql",
    "query",
    "database",
    "db",
    "migrate",
    "migrator"
  ],
  "publishConfig": {
    "access": "public"
  }
}