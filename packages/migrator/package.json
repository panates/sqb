{
  "name": "@sqb/migrator",
  "description": "Database migrator for SQB",
  "version": "4.14.0",
  "author": "Panates",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Ilker Gurelli <i.gurelli@panates.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqbjs/sqb.git",
    "directory": "packages/migrator"
  },
  "type": "module",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -b tsconfig-build-cjs.json",
    "build:esm": "tsc -b tsconfig-build-esm.json",
    "postbuild": "cp README.md package.json ../../LICENSE ../../build/migrator && cp ../../package.cjs.json ../../build/migrator/cjs/package.json",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "test": "jest",
    "cover": "jest --collect-coverage",
    "clean": "npm run clean:src | npm run clean:dist | npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/migrator",
    "clean:cover": "rimraf ../../coverage/migrator"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "strict-typed-events": "^2.4.0",
    "ts-gems": "^3.4.0"
  },
  "peerDependencies": {
    "@sqb/builder": "^4.14.0",
    "@sqb/connect": "^4.14.0",
    "@sqb/postgres": "^4.14.0"
  },
  "devDependencies": {
    "postgrejs": "^2.15.4"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "bin/",
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "sqb",
    "sql",
    "query",
    "database",
    "db",
    "migrate",
    "migrator"
  ]
}
