{
  "name": "@sqb/sqlite-dialect",
  "description": "SQB serialization extension for SQLite database",
  "version": "4.0.1-beta.9",
  "author": "Panates Ltd.",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqbjs/sqb.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "sql",
    "query",
    "builder",
    "dialect",
    "sqlite",
    "database"
  ],
  "dependencies": {},
  "devDependencies": {},
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 10.0"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "LICENSE",
    "README",
    "dist/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output"
  },
  "scripts": {
    "test": "ts-mocha -p test/tsconfig.json --paths --reporter spec test/**/*.spec.ts",
    "cover": "nyc --reporter=cobertura --reporter html --reporter text npm run test",
    "build": "tsc -b tsconfig-build.json",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint src/** --no-error-on-unmatched-pattern"
  }
}